{
  "name": "the-bomb",
  "version": "1.0.0",
  "description": "bomb countdown app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "nodemon --watch . --watch views --ext js,html,css --exec 'electron .'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acomete/the-bomb.git"
  },
  "keywords": [
    "bomb",
    "espace-game"
  ],
  "author": "Pierre-GÃ©rard David",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/acomete/the-bomb/issues"
  },
  "homepage": "https://github.com/acomete/the-bomb#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "electron": "^23.1.3"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
